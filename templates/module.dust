import protobuf = require("protobufjs");

export interface ProtoPackage extends protobuf.Message {
	toArrayBuffer(): ArrayBuffer;
	toBase64(): string;
	toString(): string;
	$type: protobuf.ReflectMessage;
}

export interface ProtoPackageBuilder extends protobuf.ProtoBuf {
	{#definitions}
	{name}: {type};
	{/definitions}
}

export class Proto {
	private static builder: ProtoPackageBuilder = (<ProtoPackageBuilder>protobuf.loadJson(decodeURIComponent("{jsonString}")).build());
	
	{#messages}
	static {name} : {name}Builder = Proto.builder.{name};
	{/messages}
}

